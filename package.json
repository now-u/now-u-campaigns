{
  "homepage": "https://now-u.com/",
  "name": "now-u-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "jest-dom": "^4.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
